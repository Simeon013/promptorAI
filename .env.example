# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe (Phase 3)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_PRICE_STARTER=price_your_starter_price_id
STRIPE_PRICE_PRO=price_your_pro_price_id
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# App URL (for SEO metadata and production)
# Development: http://localhost:3000
# Production: https://promptor.app (or your domain)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Upstash Redis (Rate Limiting - Phase 5)
# Créez un compte gratuit sur https://upstash.com
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# Google AdSense (Publicités pour utilisateurs FREE)
# 1. Créez un compte sur https://www.google.com/adsense
# 2. Ajoutez votre site et attendez l'approbation
# 3. Créez des unités publicitaires dans AdSense
# 4. Copiez votre ID client (ca-pub-XXXXXXXXXXXXXXXX)
NEXT_PUBLIC_GOOGLE_ADSENSE_ID=ca-pub-XXXXXXXXXXXXXXXX

# Brevo (Email Service - ex-Sendinblue)
# 1. Créez un compte gratuit sur https://www.brevo.com (300 emails/jour gratuits)
# 2. Allez dans Settings > SMTP & API > API Keys
# 3. Créez une nouvelle API key (v3)
# 4. Créez des listes dans Contacts > Lists (notez les IDs)
# 5. (Optionnel) Ajoutez un domaine vérifié pour production
BREVO_API_KEY=xkeysib-your_brevo_api_key
BREVO_SENDER_EMAIL=your_verified_email@example.com
BREVO_LIST_ALL_USERS=1
BREVO_LIST_FREE_USERS=2
BREVO_LIST_PRO_USERS=3
BREVO_LIST_NEWSLETTER=4
BREVO_LIST_INACTIVE_USERS=5
